<web-app xmlns = "https://jakarta.ee/xml/ns/jakartaee"
xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation= "https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
version = "5.0" metadata-complete = "true">

<context-param>
	<param-name>country</param-name>
	<param-value>Egypt</param-value>
</context-param>
<resource-ref>
	<description>DB Connection</description>
	<res-ref-name>jdbc/TestDB</res-ref-name>
	<res-type>javax.sql.DataSource</res-type>
	<res-auth>Container</res-auth>
</resource-ref>
<servlet>
	<servlet-name>Testing</servlet-name>
	<servlet-class>iti.gov.hadeer.HelloServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Testing</servlet-name>
	<url-pattern>/hello</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>ContextLab</servlet-name>
	<servlet-class>iti.gov.hadeer.ServletContextLab</servlet-class>
	
</servlet>
<servlet-mapping>
	<servlet-name>ContextLab</servlet-name>
	<url-pattern>/contextlab</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>contentType</servlet-name>
	<servlet-class>iti.gov.hadeer.ContentType</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>contentType</servlet-name>
	<url-pattern>/contentType</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>contentType2</servlet-name>
	<servlet-class>iti.gov.hadeer.ContentType2</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>contentType2</servlet-name>
	<url-pattern>/contentType2</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Headers</servlet-name>
	<servlet-class>iti.gov.hadeer.Headers</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Headers</servlet-name>
	<url-pattern>/headers</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>SendRedirect</servlet-name>
	<servlet-class>iti.gov.hadeer.SendRedirect</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>SendRedirect</servlet-name>
	<url-pattern>/sendredirect</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Welcome</servlet-name>
	<servlet-class>iti.gov.hadeer.Welcome</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Welcome</servlet-name>
	<url-pattern>/welcome</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>iti.gov.hadeer.Login</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>RegistrationForm</servlet-name>
	<servlet-class>iti.gov.hadeer.RegistrationForm</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>RegistrationForm</servlet-name>
	<url-pattern>/register</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>SearchForm</servlet-name>
	<servlet-class>iti.gov.hadeer.SearchForm</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>SearchForm</servlet-name>
	<url-pattern>/searchform</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>DrawEmpTable</servlet-name>
	<servlet-class>iti.gov.hadeer.DrawEmpTable</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>DrawEmpTable</servlet-name>
	<url-pattern>/drawemptable</url-pattern>
</servlet-mapping>

</web-app>